{
  "rewrites": [
    {
      "source": "/:username/overview",
      "destination": "/api/profile?username=:username&page=overview"
    },
    {
      "source": "/:username/projects",
      "destination": "/api/profile?username=:username&page=projects"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}